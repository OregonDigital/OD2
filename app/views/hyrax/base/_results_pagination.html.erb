<% if show_pagination? and @response.total_pages > 1 %>
 <div class="row record-padding">
  <div class="col-md-12">
    <div class="pagination">
      <%= paginate @response, :outer_window => 2, :theme => 'blacklight', :template => OregonDigital::TemplateRenderProxy.new(self) %>
    </div>
  </div>
 </div>
<% end %>
