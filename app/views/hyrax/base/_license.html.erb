<div class="license">
  <h2><%= t('.header') %></h2>
  <% license=@presenter.attribute_to_html(:license, render_as: :license, html_dl: true) %>
  <% unless license.blank? %>
    <%= license %>
  <% else %>
    <%= @presenter.attribute_to_html(:rights_statement, render_as: :rights_statement, html_dl: true) %>
  <% end %>
</div>
