/log/*
/tmp/*
!/tmp/pids/.keep
!/log/.keep
!/tmp/.keep

/node_modules
/yarn-error.log

.byebug_history

coverage/*

# Ignore local overrides
/docker-compose.override.yml

/public/system
/public/branding

/vendor/bundle
.byebug_history

config/local_env.yml

fits.log
.vscode/*

rules.log
log/rules.log
blazegraph.jnl
